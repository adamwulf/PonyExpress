      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "All notifications sent through this ",
                  "type": "text"
                },
                {
                  "code": "PostOfficeBranch",
                  "type": "codeVoice"
                },
                {
                  "text": " must conform to ",
                  "type": "text"
                },
                {
                  "code": "Notification",
                  "type": "codeVoice"
                },
                {
                  "text": ",",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "and all senders using this ",
                  "type": "text"
                },
                {
                  "code": "PostOfficeBranch",
                  "type": "codeVoice"
                },
                {
                  "text": " must conform to ",
                  "type": "text"
                },
                {
                  "code": "Sender",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        },
        {
          "inlineContent": [
            {
              "text": "Define a ",
              "type": "text"
            },
            {
              "code": "PostOfficeBranch",
              "type": "codeVoice"
            },
            {
              "text": " to more narrowly define what types of notifications can be sent from",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "specific types of senders. While observers are strictly typed in ",
              "type": "text"
            },
            {
              "identifier": "doc://PonyExpress/documentation/PonyExpress/PostOffice",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", the sent notifications",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and senders are not type constrained. Using a ",
              "type": "text"
            },
            {
              "code": "PostOfficeBranch",
              "type": "codeVoice"
            },
            {
              "text": " can add constraints to the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "notifications and senders, which helps prevent sending incorrect notification objects or senders due to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "typos or other common coding mistakes.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let myBranch = PostOfficeBranch<MySpecificEvents, MySender>()"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
